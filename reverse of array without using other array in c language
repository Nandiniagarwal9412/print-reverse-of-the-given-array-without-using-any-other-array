#include<stdio.h>
void reverse(int arr[]){
    int i=0;
    int j=4;
    while(i<j){
        int temp=arr[i];
        arr[i]=arr[j];
        arr[j]=temp;
        i++;
        j--;
    }
    for(int k=0;k<5;k++){
        printf("%d \n",arr[k]);
    }
    return;
}
int main(){
 int arr[5]={1,2,3,4,5};
 reverse(arr);
  return 0;
}
